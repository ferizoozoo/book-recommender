{
  "watch": [
    "src/"
  ],
  "ext": "ts,json",
  "ignore": [
    "node_modules",
    "dist",
    "temp_dist",
    "*.test.ts",
    "*.spec.ts"
  ],
  "execMap": {
    "ts": "node --loader ts-node/esm --experimental-specifier-resolution=node"
  },
  "env": {
    "NODE_ENV": "development",
    "TS_NODE_DEBUG": "true"
  },
  "events": {
  },
  "verbose": true
}